{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LoginResponse",
  "type": "object",
  "properties": {
    "topicName" : { "type": "string" },
    "correlatorId" : { "type": "integer" },
    "status": {
      "type": "string",
      "enum": [
        "SUCCESSFUL",
        "FAILED"
      ]
    },
    "username": {
        "type": ["string", "null"],
        "minLength": 3
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["topicName", "correlatorId", "status", "username","timestamp"]
}