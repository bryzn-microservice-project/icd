{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NewAccountResponse",
  "type": "object",
  "properties": {
    "topicName" : { "type": "string" },
    "correlatorId" : { "type": "integer" },
    "username": { "type": "string" },
    "status": {
      "type": "string",
      "enum": [
        "SUCCESSFUL",
        "FAILED"
      ]
    },
    "statusMessage": { "type": "string" },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["topicName", "correlatorId", "username", "status", "statusMessage", "timestamp"]
}